            <field name="view_mode">tree</field>
            <field name="view_id" ref="t_tree_v_dimensional_inspection"/>
            <field name="act_window_id" ref="tma_dimensional_inspection"/>
        </record>
        <record model="ir.actions.act_window.view" id="tma_dimensional_inspection_form_view">
            <field name="sequence" eval="2"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="t_form_v_dimensional_inspection"/>
            <field name="act_window_id" ref="tma_dimensional_inspection"/>
        </record>
		
