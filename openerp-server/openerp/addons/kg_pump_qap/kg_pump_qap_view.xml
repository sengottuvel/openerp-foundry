<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
	<!-- Dimensional Inspection -->
	
	<!-- Form View Start -->

		<record id="t_form_v_dimensional_inspection" model="ir.ui.view">
			<field name="name">Dimensional Inspection Entry Form</field>
			<field name="model">kg.pump.qap</field>
			<field name="arch" type="xml">
				<form string="Dimensional Inspection" version="7.0" create="false" delete="false">
					<header>
						<button name="di_update" type="object" string="Update" class="oe_highlight" confirm="Are you sure to proceed ?"
						attrs="{'invisible': [('di_state','=','completed')]}"/>
					</header>
						<group string="Dimensional Inspection">
							<group>
								<field name="di_date" attrs="{'readonly': [('di_state','=','completed')]}"/>
								<field name="di_shift_id" attrs="{'required':[('di_result','!=','reject')],'readonly': [('di_state','=','completed')]}"/>
								<field name="di_verified_by" attrs="{'required':[('di_result','!=','reject')],'readonly': [('di_state','=','completed')]}"/>
								<field name="di_reject_remarks_id" attrs="{'readonly': [('di_state','=','completed')],'invisible':[('di_result','!=','reject')],'required':[('di_result','=','reject')]}"/>
								<field name="di_remarks" attrs="{'readonly': [('di_state','=','completed')]}"/>
							</group>											
							<group>
								<field name="qap_plan_id" />
								<field name="di_operator" attrs="{'required':[('di_result','!=','reject')],'readonly': [('di_state','=','completed')]}"/>
								<field name="di_result" attrs="{'required':[('di_result','!=','reject')],'readonly': [('di_state','=','completed')]}"/>
								<field name="flag_di_customer_specific" attrs="{'readonly': [('di_state','=','completed')]}"/>
								<field name="overall_dim" required="1" attrs="{'readonly': [('di_state','=','completed')]}"/>
								<field name="di_state" invisible="1"/>
							</group>					
						</group>
						<group string="WO Details">
							<group>
								<field name="order_no"/>
								<field name="pump_model_id"/>
								<field name="moc_construction_id"/>
							</group>											
							<group>
								<field name="order_category"/>
								<field name="pump_serial_no"/>
							</group>					
						</group>
						<notebook>
							<page string="Dimentional Details">
								<field name="line_ids" colspan="4" nolabel="1">
									<tree string="Dimentional Details" editable="top" create="false" delete="false">
									   <field name="dimentional_details" />
									   <field name="min_weight"/>
									   <field name="max_weight" />
									   <field name="actual_weight" required="1"/>
									   <field name="remarks" widget="char"/>							  
									</tree>
								</field>
							</page>
							<page string="Entry Info">
								<group>
									<field name="company_id"/>
								</group>
								<group>
									<field name="active"/>
								</group>
								<group>
									<group>
										<field name="user_id"/>
										<field name="update_user_id"/>
										
									</group>
									<group>
										<field name="crt_date"/>
										<field name="update_date"/>
									</group>
								</group>
							</page>
<!--
							<page string="Notification Info">								
								<group>
									<group>
										<field name="flag_sms"/>										
									</group>
									<group>
										<field name="flag_email"/>
										<field name="flag_spl_approve"/>
									</group>
								</group>
							</page>
-->
						</notebook>
					</form>
			</field>
		</record>
		
	<!-- Form View End -->
		
	
	<!-- Tree View Start -->
		
		<record id="t_tree_v_dimensional_inspection" model="ir.ui.view">
			<field name="name">Dimensional Inspection Entry Tree</field>
			<field name="model">kg.pump.qap</field>
			<field eval="8" name="priority"/>
			<field name="arch" type="xml">
				<tree toolbar="True" string="Dimensional Inspection" create="false" delete="false">					
					<field name="order_no"/>
					<field name="order_category"/>
					<field name="pump_model_id"/>
					<field name="pump_serial_no"/>
					<field name="moc_construction_id"/>
					<field name="di_result"/>
				</tree>
			</field>
		</record>
		
	<!-- Tree View End -->
		
	<!-- Search View Start -->

		<record id="t_search_v_dimensional_inspection" model="ir.ui.view">
            <field name="name">Dimensional Inspection Entry Search</field>
            <field name="model">kg.pump.qap</field>
            <field name="arch" type="xml">
                <search string="Dimensional Inspection Entry Search">
					
					<field name="pump_model_id" string="Pump Model"  filter_domain="[('pump_model_id','ilike',self)]" />
					<field name="pump_serial_no" string="Pump Serial No."  filter_domain="[('pump_serial_no','ilike',self)]" />
					<field name="order_no" string="Work Order No."  filter_domain="[('order_no','ilike',self)]" />
					<field name="moc_construction_id" string="MOC Construction"  filter_domain="[('moc_id','ilike',self)]" />				
					<separator/>
                    <group expand="0" string="Group By...">
                        <filter string="WO No." icon="terp-gtk-select-all" domain="[]" context="{'group_by':'order_no'}"/>
						<filter string="Category" icon="terp-gtk-select-all" domain="[]" context="{'group_by':'order_category'}"/>
						<filter string="Pump Model" icon="terp-gtk-select-all" domain="[]" context="{'group_by':'pump_model_id'}"/>
						<filter string="Pump Serial No." icon="terp-gtk-select-all" domain="[]" context="{'group_by':'pump_serial_no'}"/>
						<filter string="MOC Construction" icon="terp-gtk-select-all" domain="[]" context="{'group_by':'moc_construction_id'}"/>
						<filter string="Result" icon="terp-gtk-select-all" domain="[]" context="{'group_by':'di_result'}"/>
					</group>
				</search>
			</field>
		</record>
		
	<!-- Search View End -->		
			
        
	<!-- Menu Action Start-->
		
		<record id="tma_dimensional_inspection" model="ir.actions.act_window">
			<field name="name">Dimensional Inspection</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">kg.pump.qap</field>
			<field name="view_type">form</field>
			<field name="context">{}</field>
			<field name="search_view_id" ref="t_search_v_dimensional_inspection"/>
			<field name="domain">[('test_state','=','di'),('di_state','=','pending')]</field>
		</record>
		
		<record model="ir.actions.act_window.view" id="tma_dimensional_inspection_tree_view">
