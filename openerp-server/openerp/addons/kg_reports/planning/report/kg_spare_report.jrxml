	<subDataset name="dataset3">
		<parameter name="header_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT company_name,date,wo_no,pump_type,trimming_dia,flange_name,pattern_check,position_no,drawing_no,
case
when mtl_form = 'AFoundry' then 'Foundry'
when mtl_form = 'BOT' then 'BOT'
when mtl_form = 'MS' then 'MS'
else
'-' end as mtl_form,
material_code,pattern_no,csd_no,pattern_name,moc_name,weight_kg,qty,uom,sequence_no,
 case when ((row_number() OVER (PARTITION by sequence_no)) = 1 ) then sequence_no else null end as ref_seq_no,
remarks from (
SELECT
company.name as company_name,
to_char(CURRENT_DATE::date,'dd-mm-YYYY') as date,
work_order.order_no as wo_no,
pump_type.name as pump_type,
work_order.trimming_dia as trimming_dia,
flange.name as flange_name,
(CASE WHEN ch_bom.flag_pattern_check = False
THEN '-'
WHEN ch_bom.flag_pattern_check = True
THEN 'Yes'
ELSE ''
end ) as pattern_check,
position_num.name as position_no,
case
when position_num.drawing_no is null then '-'
else position_num.drawing_no end as drawing_no,
'AFoundry' as mtl_form,
case
when ch_bom.material_code is null then '-'
else ch_bom.material_code end as material_code,
pattern.name as pattern_no,
case
when pattern.csd_code is null then '-'
else pattern.csd_code end as csd_no,
pattern.pattern_name as pattern_name,
moc.name as moc_name,
trim(TO_CHAR(ch_bom.weight::float, '999G999G99G999G99G99G990D99'))  as weight_kg,
ch_bom.qty as qty,
'Nos' as uom,
ch_bom.sequence_no as sequence_no,
ch_bom.add_spec as remarks

FROM ch_work_order_details work_order

left join ch_order_bom_details ch_bom on (ch_bom.header_id = work_order.id)
left join ch_pumpseries_flange flange on (flange.id = work_order.flange_standard)
left join kg_pattern_master pattern on (pattern.id = ch_bom.pattern_id)
left join kg_position_number position_num on (position_num.id = ch_bom.position_id)
left join kg_pumpmodel_master pump_type on (pump_type.id = work_order.pump_model_id)
left join kg_moc_master moc on (moc.id = ch_bom.moc_id)
left join res_company company on (company.id = pattern.company_id)

where ch_bom.flag_applicable = True and

work_order.id = $P{header_id}

UNION

SELECT

company.name as company_name,
to_char(CURRENT_DATE::date,'dd-mm-YYYY') as date,
work_order.order_no as wo_no,
pump_type.name as pump_type,
work_order.trimming_dia as trimming_dia,
flange.name as flange_name,
'-' as pattern_check,
position_num.name as position_no,
case
when position_num.drawing_no is null then '-'
else position_num.drawing_no end as drawing_no,
'MS' as mtl_form,
case
when ch_machineshop.material_code is null then '-'
else ch_machineshop.material_code end as material_code,
machine.code as pattern_no,
case
when machine.csd_code is null then '-'
else machine.csd_code end as csd_no,
machine.name as pattern_name,
moc.name as moc_name,

trim(TO_CHAR('0.00'::float, '999G999G99G999G99G99G990D99'))  as weight_kg,

ch_machineshop.qty as qty,
'Nos' as uom,
ch_machineshop.sequence_no as sequence_no,
ch_machineshop.remarks as remarks

FROM ch_work_order_details work_order

left join ch_order_machineshop_details ch_machineshop on (ch_machineshop.header_id = work_order.id)
left join kg_machine_shop machine on (machine.id = ch_machineshop.ms_id)
left join ch_pumpseries_flange flange on (flange.id = work_order.flange_standard)
left join kg_position_number position_num on (position_num.id = ch_machineshop.position_id)
left join kg_pumpmodel_master pump_type on (pump_type.id = work_order.pump_model_id)
left join kg_moc_master moc on (moc.id = ch_machineshop.moc_id)
left join res_company company on (company.id = machine.company_id)


where ch_machineshop.flag_applicable = True and

work_order.id = $P{header_id}


UNION

SELECT

company.name as company_name,
to_char(CURRENT_DATE::date,'dd-mm-YYYY') as date,
work_order.order_no as wo_no,
pump_type.name as pump_type,
work_order.trimming_dia as trimming_dia,
flange.name as flange_name,
'-' as pattern_check,
'-' as position_no,
'-' as drawing_no,
'BOT' as mtl_form,
case
when ch_bot.material_code is null then '-'
else ch_bot.material_code end as material_code,
machine.code as pattern_no,
case
when machine.csd_code is null then '-'
else machine.csd_code end as csd_no,
machine.name as pattern_name,
moc.name as moc_name,

trim(TO_CHAR('0.00'::float, '999G999G99G999G99G99G990D99'))  as weight_kg,

ch_bot.qty as qty,
'Nos' as uom,
ch_bot.sequence_no as sequence_no,
ch_bot.remarks as remarks

FROM ch_work_order_details work_order

left join ch_order_bot_details ch_bot on (ch_bot.header_id = work_order.id)
left join kg_machine_shop machine on (machine.id = ch_bot.bot_id)
left join ch_pumpseries_flange flange on (flange.id = work_order.flange_standard)
left join kg_pumpmodel_master pump_type on (pump_type.id = work_order.pump_model_id)
left join kg_moc_master moc on (moc.id = ch_bot.moc_id)
left join res_company company on (company.id = machine.company_id)


where ch_bot.flag_applicable = True and

work_order.id = $P{header_id}

order by sequence_no,mtl_form
) as sample]]>
		</queryString>
