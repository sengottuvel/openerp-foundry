	<parameter name="type" class="java.lang.Integer"/>
	<parameter name="printed_by" class="java.lang.String"/>
	<parameter name="print_date" class="java.lang.String"/>
	<parameter name="wiz_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
wo_line.id as id,
wo.order_category as order_category,

case when (ROW_NUMBER()OVER(PARTITION By wo_line.id)=COUNT(wo_line.id)OVER(PARTITION By wo_line.id)) then
	 'stop' end as brkneed,

case when wo_line.order_category = 'spare' then 1
when wo_line.order_category = 'access' then 2 end as categ_type,

case
when wo.drawing_approval = 'yes' then 'Yes'
when wo.drawing_approval = 'no' then 'No'
else '-' end as drawing_approval,

case
when wo.inspection = 'yes' then 'Yes'
when wo.inspection = 'no' then 'No'
when wo.inspection = 'tpi' then 'TPI'
when wo.inspection = 'customer' then 'Customer'
when wo.inspection = 'consultant' then 'Consultant'
when wo.inspection = 'stagewise' then 'Stage wise'
else '-' end as inspection,

case
when wo.road_permit = 'yes' then 'Yes'
when wo.road_permit = 'no' then 'No'
else '-' end as road_permit,
case
when wo.l_d_clause = '5_1' then '0.5 - 1.0% of total order value'
when wo.l_d_clause = '1_10' then '1 to 10% of total order value'
when wo.l_d_clause = 'nill' then 'Nill'
else '-' end as l_d_clause,

case
when wo.excise_duty = 'inclusive' then 'Inclusive'
when wo.excise_duty = 'extra' then 'Extra'
when wo.excise_duty = 'exemted' then 'Exemted - Export'
when wo.excise_duty = 'pac' then 'PAC'
when wo.excise_duty = 'sez' then 'SEZ'
when wo.excise_duty = 'ct1' then 'CT1'
when wo.excise_duty = 'ct3' then 'CT3'
else '-' end as excise_duty,




wo_line.order_category as line_order_category,

case when
wo.order_priority = 'normal' then 'Normal'
else 'Emergency' end as order_priority,

wo_line.order_no as wo_ref,
cust.name as customer_name,
pump.name as model,
case when
wo_line.pump_model_type = 'horizontal' then 'Horizontal'
else 'Veritcal' end as pump_type,

case when pump_crm.description is not null then pump_crm.description else '-' end as description,
coalesce(pump_crm.equipment_no,'-') as tag_no,
wo_line.qty as qty,
coalesce(pump_crm.size_suctionx,'-') as size,
case when pump_crm.flange_standard > 0 then pump_crm.flange_standard::text else '-' end as flange_standard,
coalesce(liquid.name,'-') as liquid_handled,
case when pump_crm.specific_gravity > 0 then pump_crm.specific_gravity::text else '-' end as specific_gravity,
case when pump_crm.temperature_in_c is not null then pump_crm.temperature_in_c::text else '-' end as temperature_in_c,
case when pump_crm.capacity_in_liquid > 0 then pump_crm.capacity_in_liquid::text else '-' end as capacity_in_m3hr,
case when pump_crm.head_in_liquid > 0 then pump_crm.head_in_liquid::text else '-' end as head_in_m,
case when pump_crm.efficiency_in > 0 then pump_crm.efficiency_in::text else '-' end as efficiency,
case when pump_crm.motor_make > 0 then pump_crm.motor_make::text else '-' end as motor_make,
case when pump_crm.full_load_rpm > 0 then pump_crm.full_load_rpm::text else '-' end as speed_rpm,
case when pump_crm.lubrication_type = 'grease' then 'Grease'
when pump_crm.lubrication_type = 'oil' then 'Oil'
else '-' end as lubrication_type,
case when pump_crm.npsh_r_m > 0 then pump_crm.npsh_r_m::text else '-' end as npsh_r_m,
case when pump_crm.shaft_sealing = 'gld_packing_tiga' then 'Gland Packing-TIGA'
when pump_crm.shaft_sealing = 'gld_packing_ptfe' then 'Gland Packing-PTFE'
when pump_crm.shaft_sealing = 'mc_seal' then 'M/C Seal'
when pump_crm.shaft_sealing = 'dynamic_seal' then 'Dynamic Seal'
else '-' end as shaft_sealing,
coalesce(brand.name,'-') as mech_seal_make,

case
when pump_crm.seal_type = 'sums' then 'Single Unbalanced Multiple Spring'
when pump_crm.seal_type = 'suss' then 'Single Unbalanced Single Spring'
when pump_crm.seal_type = 'sbsm' then 'Single Balanced Spring Stationary Mounted'
when pump_crm.seal_type = 'cs' then 'Cartridge Seal'
when pump_crm.seal_type = 'sbms' then 'Single Balanced Multiple Spring'
when pump_crm.seal_type = 'sbss' then 'Single Balanced Single Spring'
when pump_crm.seal_type = 'sc' then 'Single Cartridge'
when pump_crm.seal_type = 'dubtb' then 'Double Unbalanced Back to Back'
when pump_crm.seal_type = 'dbbtb' then 'Double Balanced Back to Back'
when pump_crm.seal_type = 'ts' then 'Tandem Seal'
when pump_crm.seal_type = 'dc' then 'Double Cartridge'
when pump_crm.seal_type = 'drsu' then 'Dry Running - Single Unbalanced'
when pump_crm.seal_type = 'mbi' then 'Metallic Bellow Inside'
when pump_crm.seal_type = 'tbs' then 'Teflon Bellow Seal-Outside Mounted Dry Running'
else '-' end as seal_type,

case
when pump_crm.face_combination = 'c_vs_sic' then 'C VS SIC'
when pump_crm.face_combination = 'sic_vs_sic' then 'SIC VS SIC'
when pump_crm.face_combination = 'c_vs_sic' then 'SIC VS SIC / C VS SIC'
when pump_crm.face_combination = 'gft_vs_ceramic' then 'GFT VS CERAMIC'
else '-' end as face_combination,

coalesce(pump_crm.api_plan,'-') as api_plan,
case when pump_crm.shut_off_head > 0 then pump_crm.shut_off_head::text else '-' end as shut_off_head,
case when pump_crm.impeller_dia_min > 0 then pump_crm.impeller_dia_min::text else '-' end as impeller_dia_min,
case when pump_crm.hydrostatic_test_pressure > 0 then pump_crm.hydrostatic_test_pressure::text else '-' end as hydrostatic_test_pressure,

to_char(wo_line.delivery_date::date,'dd-mm-YYYY') as due_date,
to_char(wo.entry_date::date,'dd-mm-YYYY') as order_date,
comp.name as company_name,
wo_line.note as remarks,
to_char(CURRENT_DATE::date,'dd-mm-YYYY') as date,
to_char(CURRENT_TIMESTAMP, 'DD-MM-YYYY HH12:MI:SS AM') AS new_Date,
pump_crm.s_no as serial_no,
dealer.name as delear

from ch_work_order_details wo_line
left join kg_pumpmodel_master pump on pump.id = wo_line.pump_model_id
left join ch_kg_crm_pumpmodel pump_crm on pump_crm.id = wo_line.enquiry_line_id
left join kg_crm_enquiry crm_enquiry on crm_enquiry.id = pump_crm.header_id
left join kg_fluid_master liquid on liquid.id = pump_crm.fluid_id
left join kg_work_order wo on wo.id = wo_line.header_id
left join res_partner cust on cust.id = wo.partner_id
left join res_partner dealer on dealer.id = crm_enquiry.dealer_id
left join res_company comp on comp.id = wo.company_id
left join kg_brand_master brand on brand.id = pump_crm.mech_seal_make

where case
when (select order_id from m2m_work_order_spare_print_report_details where foundry_wiz_id =$P{wiz_id}  limit 1) is not null then wo_line.id in (select order_id from m2m_work_order_spare_print_report_details where foundry_wiz_id = $P{wiz_id})
else
wo_line.id in (select  id from ch_work_order_details) end
and wo_line.order_category ='spare' order by wo_line.order_no]]>
	</queryString>
