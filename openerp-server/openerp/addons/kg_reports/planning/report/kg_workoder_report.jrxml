	<subDataset name="Spare dataset">
		<parameter name="header_id" class="java.lang.Integer"/>
		<parameter name="category" class="java.lang.String"/>
		<queryString>
			<![CDATA[select

pump.name as pump_model,
foundry_line.pattern_name,
foundry_line.qty,
foundry_line.add_spec as oth_spec


from ch_order_bom_details foundry_line

left join kg_pumpmodel_master pump on pump.id = foundry_line.pump_model_id

where foundry_line.header_id = $P{header_id} and foundry_line.order_category = 'spare'  and foundry_line.flag_applicable = 't'

union

select

pump.name as pump_model,
ms_line.name,
ms_line.qty,
ms_line.remarks as oth_spec


from ch_order_machineshop_details ms_line

left join ch_work_order_details wo_line on wo_line.id = ms_line.header_id
left join kg_pumpmodel_master pump on pump.id = wo_line.pump_model_id

where ms_line.header_id = $P{header_id} and wo_line.order_category = 'spare'
and ms_line.flag_applicable = 't'

union

select

pump.name as pump_model,
bot_line.item_name,
bot_line.qty,
bot_line.remarks as oth_spec


from ch_order_bot_details bot_line

left join ch_work_order_details wo_line on wo_line.id = bot_line.header_id
left join kg_pumpmodel_master pump on pump.id = wo_line.pump_model_id

where bot_line.header_id = $P{header_id} and wo_line.order_category = 'spare'
and bot_line.flag_applicable = 't']]>
		</queryString>
		<field name="pump_model" class="java.lang.String"/>
		<field name="pattern_name" class="java.lang.String"/>
		<field name="qty" class="java.lang.Integer"/>
		<field name="oth_spec" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Material dataset">
		<parameter name="header_id" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select offer_mat.name as mat,moc.name as moc from ch_moc_construction moc_const


left join kg_offer_materials offer_mat on offer_mat.id = moc_const.offer_id
left join kg_moc_master moc on  moc.id = moc_const.moc_id

where moc_const.header_id

in
(
select pump_offer.enquiry_line_id
from ch_work_order_details wo_line

left join ch_pump_offer pump_offer on pump_offer.id = wo_line.pump_offer_line_id

where wo_line.id = $P{header_id}
)]]>
		</queryString>
		<field name="mat" class="java.lang.String"/>
		<field name="moc" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Accessories Dataset">
		<parameter name="header_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select

access.name,
'Yes' as yes,
wo_access.oth_spec as oth_spec

from ch_wo_accessories wo_access

left join kg_accessories_master access on access.id = wo_access.access_id
left join ch_work_order_details wo_line on wo_line.id = wo_access.header_id

where wo_access.header_id = $P{header_id}]]>
		</queryString>
		<field name="name" class="java.lang.String"/>
		<field name="yes" class="java.lang.String"/>
		<field name="oth_spec" class="java.lang.String"/>
	</subDataset>
	<subDataset name="dataset1"/>
	<parameter name="type" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="printed_by" class="java.lang.String"/>
	<parameter name="print_date" class="java.lang.String"/>
	<parameter name="wiz_id" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
wo_line.id as id,
wo.order_category as order_category,
case
when wo.drawing_approval = 'yes' then 'Yes'
when wo.drawing_approval = 'no' then 'No'
else '-' end as drawing_approval,

case
when wo.inspection = 'yes' then 'Yes'
when wo.inspection = 'no' then 'No'
when wo.inspection = 'tpi' then 'TPI'
when wo.inspection = 'customer' then 'Customer'
when wo.inspection = 'consultant' then 'Consultant'
when wo.inspection = 'stagewise' then 'Stage wise'
else '-' end as inspection,

case
when wo.road_permit = 'yes' then 'Yes'
when wo.road_permit = 'no' then 'No'
else '-' end as road_permit,
case
when wo.l_d_clause = '5_1' then '0.5 - 1.0% of total order value'
when wo.l_d_clause = '1_10' then '1 to 10% of total order value'
when wo.l_d_clause = 'nill' then 'Nill'
else '-' end as l_d_clause,

case
when wo.excise_duty = 'inclusive' then 'Inclusive'
when wo.excise_duty = 'extra' then 'Extra'
when wo.excise_duty = 'exemted' then 'Exemted - Export'
when wo.excise_duty = 'pac' then 'PAC'
when wo.excise_duty = 'sez' then 'SEZ'
when wo.excise_duty = 'ct1' then 'CT1'
when wo.excise_duty = 'ct3' then 'CT3'
else '-' end as excise_duty,
case
when wo_line.order_category = 'spare' and pump_crm.acces = 'yes' then 3
when wo_line.order_category = 'spare' then 1
when wo_line.order_category = 'access' then 2
when pump_crm.acces = 'yes' then 2
end as categ_type,

wo_line.order_category as line_order_category,

case when
wo.order_priority = 'normal' then 'Normal'
else 'Emergency' end as order_priority,

wo_line.order_no as wo_ref,
cust.name as customer_name,
pump.name as model,
case when
wo_line.pump_model_type = 'horizontal' then 'Horizontal'
else 'Veritcal' end as pump_type,

case when pump_crm.description is not null then pump_crm.description else '-' end as description,
coalesce(pump_crm.equipment_no,'-') as tag_no,
coalesce(pump_crm.ph_value,'-') as ph_value,
case when pump_crm.consistency > 0 then pump_crm.consistency::text else '-' end as consistency,
wo_line.qty as qty,
coalesce(pump_crm.size_suctionx,'-') as size,

case when flange.name is not null then flange.name::text else '-' end as flange_standard,

coalesce(liquid.name,'-') as liquid_handled,
case when pump_crm.specific_gravity > 0 then round(cast(pump_crm.specific_gravity as numeric),2)::text else '-' end as specific_gravity,
case when pump_crm.temperature_in_c is not null then pump_crm.temperature_in_c::text else '-' end as temperature_in_c,
case when pump_crm.capacity_in_liquid > 0 then pump_crm.capacity_in_liquid::text else '-' end as capacity_in_m3hr,
case when pump_crm.head_in_liquid > 0 then round(cast(pump_crm.head_in_liquid as numeric),2)::text else '-' end as head_in_m,
case when pump_crm.efficiency_in > 0 then pump_crm.efficiency_in::text else '-' end as efficiency,
case when pump_crm.motor_kw > 0 then round(cast(pump_crm.motor_kw as numeric),2)::text else '-' end as motor_kw,
case when brand.name is not null then brand.name::text else '-' end as motor_make,


case when pump_crm.speed_in_rpm > 0 then pump_crm.speed_in_rpm::text else '-' end as speed_pump,
case when pump_crm.speed_in_motor > 0 then pump_crm.speed_in_motor::text else '-' end as speed_motor,


case
when type_of_drive = 'motor_direct' then 'Direct'
when type_of_drive = 'belt_drive' then 'Belt drive'
when type_of_drive = 'fc_gb' then 'Fluid Coupling Gear Box'
else
'-' end as type_of_drive,

case when pump_crm.lubrication_type = 'grease' then 'Grease'
when pump_crm.lubrication_type = 'oil' then 'Oil'
else '-' end as lubrication_type,
case when pump_crm.npsh_r_m > 0 then round(cast(pump_crm.npsh_r_m as numeric),2)::text else '-' end as npsh_r_m,
case when pump_crm.shaft_sealing = 'gld_packing_tiga' then 'Gland Packing-TIGA'
when pump_crm.shaft_sealing = 'gld_packing_ptfe' then 'Gland Packing-PTFE'
when pump_crm.shaft_sealing = 'mc_seal' then 'M/C Seal'
when pump_crm.shaft_sealing = 'dynamic_seal' then 'Dynamic Seal'
else '-' end as shaft_sealing,
coalesce(brand.name,'-') as mech_seal_make,

case
when pump_crm.seal_type = 'sums' then 'Single Unbalanced Multiple Spring'
when pump_crm.seal_type = 'suss' then 'Single Unbalanced Single Spring'
when pump_crm.seal_type = 'sbsm' then 'Single Balanced Spring Stationary Mounted'
when pump_crm.seal_type = 'cs' then 'Cartridge Seal'
when pump_crm.seal_type = 'sbms' then 'Single Balanced Multiple Spring'
when pump_crm.seal_type = 'sbss' then 'Single Balanced Single Spring'
when pump_crm.seal_type = 'sc' then 'Single Cartridge'
when pump_crm.seal_type = 'dubtb' then 'Double Unbalanced Back to Back'
when pump_crm.seal_type = 'dbbtb' then 'Double Balanced Back to Back'
when pump_crm.seal_type = 'ts' then 'Tandem Seal'
when pump_crm.seal_type = 'dc' then 'Double Cartridge'
when pump_crm.seal_type = 'drsu' then 'Dry Running - Single Unbalanced'
when pump_crm.seal_type = 'mbi' then 'Metallic Bellow Inside'
when pump_crm.seal_type = 'tbs' then 'Teflon Bellow Seal-Outside Mounted Dry Running'
else '-' end as seal_type,

case
when pump_crm.face_combination = 'c_vs_sic' then 'C VS SIC'
when pump_crm.face_combination = 'sic_vs_sic' then 'SIC VS SIC'
when pump_crm.face_combination = 'c_vs_sic' then 'SIC VS SIC / C VS SIC'
when pump_crm.face_combination = 'gft_vs_ceramic' then 'GFT VS CERAMIC'
else '-' end as face_combination,

coalesce(pump_crm.api_plan,'-') as api_plan,
case when pump_crm.shut_off_head > 0 then round(cast(pump_crm.shut_off_head as numeric),2)::text else '-' end as shut_off_head,
case when pump_crm.impeller_dia_min > 0 then pump_crm.impeller_dia_min::text else '-' end as impeller_dia_min,
case when pump_crm.hydrostatic_test_pressure > 0 then round(cast(pump_crm.hydrostatic_test_pressure as numeric),2)::text else '-' end as hydrostatic_test_pressure,

to_char(wo_line.delivery_date::date,'dd-mm-YYYY') as due_date,
to_char(wo.entry_date::date,'dd-mm-YYYY') as order_date,
comp.name as company_name,
wo_line.note as remarks,
pump_crm.bkw_liq as bkw,

coalesce(pump_crm_wo_line.order_no,'-') as wo_name

from ch_work_order_details wo_line

left join kg_pumpmodel_master pump on pump.id = wo_line.pump_model_id
left join ch_kg_crm_pumpmodel pump_crm on pump_crm.id = wo_line.enquiry_line_id
left join ch_work_order_details pump_crm_wo_line on pump_crm.wo_line_id = pump_crm_wo_line.id
left join kg_fluid_master liquid on liquid.id = pump_crm.fluid_id
left join kg_work_order wo on wo.id = wo_line.header_id
left join res_partner cust on cust.id = wo.partner_id
left join res_company comp on comp.id = wo.company_id
left join kg_brand_master brand on brand.id = pump_crm.mech_seal_make
left join ch_pumpseries_flange flange on flange.id = pump_crm.flange_standard

where wo_line.id in (select order_id from m2m_work_order_print_report_details
where foundry_wiz_id = $P{wiz_id} )]]>
	</queryString>
