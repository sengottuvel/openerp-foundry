	<queryString>
		<![CDATA[select count,row_number,new_row_number,order_no,company_name,date,pump_model,qty,order_amount,total_amt,remarks,
from_date,to_date,
to_char((select max(entry_date) from kg_machineshop where order_line_id = line_id)::date,'dd-mm-YYYY') as cc_date,
(select

trim(TO_CHAR((COALESCE((sum((work_order.unit_price * work_order.qty)) OVER ()),0.00) ), '999G999G99G999G99G99G990D99')) as cum_total_amt

from
ch_work_order_details work_order
left join kg_pumpmodel_master pump on (work_order.pump_model_id = pump.id)
left join res_company company on (company.id = pump.company_id)

 where work_order.id in (

select order_line_id from (

select

distinct order_line_id,
Sum(case when type = 'order' then sum else 0 end) over(partition by order_line_id) as order_count,
Sum(case when type = 'machine' then sum else 0 end) over(partition by order_line_id) as machine_count

from (

select count(order_bomline_id) as sum,order_line_id,'machine' as type from kg_machineshop  where order_bomline_id in (
select id

from ch_order_bom_details where flag_applicable = 't'

order by id)
and entry_date >= month_start::date  and entry_date <= month_end::date
group by order_line_id

union

select count(id) as sum,header_id,'order' as type

from ch_order_bom_details where flag_applicable = 't'
and header_id in (select order_line_id from kg_machineshop  where order_bomline_id in (
select id

from ch_order_bom_details where flag_applicable = 't'

order by id)
and entry_date >= month_start::date and entry_date <=month_end::date )
group by header_id,type
) as a
order by order_line_id ) as b

where order_count = machine_count) limit 1 ) as cum_total_amt

from (



select
count(*) OVER () as count,
row_number() OVER () as row_number,
row_number() OVER () as new_row_number,
work_order.order_no as order_no,
company.name as company_name,
to_char(CURRENT_DATE::date,'dd-mm-YYYY') as date,
pump.name as pump_model,
work_order.qty as qty,
trim(TO_CHAR(work_order.unit_price * work_order.qty::float, '999G999G99G999G99G99G990D99'))  as order_amount,
trim(TO_CHAR((COALESCE((sum((work_order.unit_price * work_order.qty)) OVER ()),0.00) ), '999G999G99G999G99G99G990D99')) as total_amt,
work_order.note as remarks,
(select to_char(from_date,'dd-mm-yyyy') from kg_casting_completion where id =$P{wiz_id} ) as from_date,
(select to_char(to_date,'dd-mm-yyyy') from kg_casting_completion where id =$P{wiz_id} ) as to_date,
work_order.id as line_id,
(select to_char((date_trunc('month', current_date))::date,'YYYY-mm-dd')) as month_start, (to_char((date_trunc('month', current_date)+'1month'::interval-'1day'::interval)::date,'YYYY-mm-dd')) as month_end

from
ch_work_order_details work_order
left join kg_pumpmodel_master pump on (work_order.pump_model_id = pump.id)
left join res_company company on (company.id = pump.company_id)

 where work_order.id in (

select order_line_id from (

select

distinct order_line_id,
Sum(case when type = 'order' then sum else 0 end) over(partition by order_line_id) as order_count,
Sum(case when type = 'machine' then sum else 0 end) over(partition by order_line_id) as machine_count



from (


select count(order_bomline_id) as sum,order_line_id,'machine' as type from kg_machineshop  where order_bomline_id in (
select id

from ch_order_bom_details where flag_applicable = 't'

order by id)
and entry_date >= $P{from_date}::date and entry_date <= $P{to_date}::date
group by order_line_id

union

select count(id) as sum,header_id,'order' as type

from ch_order_bom_details where flag_applicable = 't'
and header_id in (select order_line_id from kg_machineshop  where order_bomline_id in (
select id

from ch_order_bom_details where flag_applicable = 't'

order by id)
and entry_date::date >=  $P{from_date}::date and entry_date <= $P{to_date}::date)
group by header_id,type
) as a
order by order_line_id ) as b

where order_count = machine_count) ) as bb order by cc_date]]>
	</queryString>
