else '-' end) as vendor_ref_no_5,
(case when entry_header.vendor_reference_date_vendor_1 is not null then to_char(entry_header.vendor_reference_date_vendor_1, 'dd/mm/yyyy')
else '-' end) as vendor_ref_date_1,
(case when entry_header.vendor_reference_date_vendor_2 is not null then to_char(entry_header.vendor_reference_date_vendor_2, 'dd/mm/yyyy')
else '-' end) as vendor_ref_date_2,
(case when entry_header.vendor_reference_date_vendor_3 is not null then to_char(entry_header.vendor_reference_date_vendor_3, 'dd/mm/yyyy')
else '-' end) as vendor_ref_date_3,
