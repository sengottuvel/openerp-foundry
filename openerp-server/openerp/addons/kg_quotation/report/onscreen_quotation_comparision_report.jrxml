	<subDataset name="product_details">
		<parameter name="comparision_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select

prod.name_template as product,
brand.name as brand,
moc.name as moc,
uom.name as uom,
entry_line.quotation_qty as qty,

round(LEAST(NULLIF(entry_line.vendor_1_price_input,0),NULLIF(entry_line.vendor_2_price_input,0),NULLIF(entry_line.vendor_3_price_input,0)
,NULLIF(entry_line.vendor_4_price_input,0),NULLIF(entry_line.vendor_5_price_input,0)
),2) as min_value,

round(entry_line.vendor_1_price_input,2) as vendor_1_rate,
round(entry_line.vendor_2_price_input,2) as vendor_2_rate,
round(entry_line.vendor_3_price_input,2) as vendor_3_rate,
round(entry_line.vendor_4_price_input,2) as vendor_4_rate,
round(entry_line.vendor_5_price_input,2) as vendor_5_rate,
round(entry_line.vendor_1_value,2) as vendor_1_value,
round(entry_line.vendor_2_value,2) as vendor_2_value,
round(entry_line.vendor_3_value,2) as vendor_3_value,
round(entry_line.vendor_4_value,2) as vendor_4_value,
round(entry_line.vendor_5_value,2) as vendor_5_value

from kg_quotation_entry_lines entry_line

left join product_product prod on(prod.id=entry_line.product_id)
left join product_uom uom on(uom.id=entry_line.uom)
left join kg_brand_master brand on(brand.id=entry_line.brand_id)
left join kg_moc_master moc on(moc.id=entry_line.moc_id)

where entry_line.header_id = $P{comparision_id}]]>
		</queryString>
