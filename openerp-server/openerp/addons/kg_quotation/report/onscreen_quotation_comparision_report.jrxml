(case when entry_header.comparison_remarks is not null then entry_header.comparison_remarks
else ' ' end) as remarks

from kg_quotation_entry_header entry_header

left join res_company company on(company.id=entry_header.company_id)
left join res_partner comppart on(comppart.id=company.partner_id)
left join res_country_state compstate on(compstate.id=comppart.state_id)
