(case when entry_header.vendor_4_freight is not null then entry_header.vendor_4_freight else '-' end) as vendor_4_freight,
(case when entry_header.vendor_4_tax is not null then entry_header.vendor_4_tax else '-' end) as vendor_4_tax,
