else '-' end) as vendor_ref_date_4,
(case when entry_header.vendor_reference_date_vendor_5 is not null then to_char(entry_header.vendor_reference_date_vendor_5, 'dd/mm/yyyy')
else '-' end) as vendor_ref_date_5,
entry_header.quotation_validity_vendor_1 as quotation_validity_1,
entry_header.quotation_validity_vendor_2 as quotation_validity_2,
entry_header.quotation_validity_vendor_3 as quotation_validity_3,
entry_header.quotation_validity_vendor_4 as quotation_validity_4,
entry_header.quotation_validity_vendor_5 as quotation_validity_5,

(case when entry_header.enquiry_type_vendor_1 = 'phone' then 'Phone' when entry_header.enquiry_type_vendor_1 = 'email' then 'Email'
when entry_header.enquiry_type_vendor_1 = 'fax' then 'Fax' when entry_header.enquiry_type_vendor_1 = 'others' then 'Others'
else '-' end) as enquiry_type_vendor_1,
(case when entry_header.enquiry_type_vendor_2 = 'phone' then 'Phone' when entry_header.enquiry_type_vendor_2 = 'email' then 'Email'
when entry_header.enquiry_type_vendor_2 = 'fax' then 'Fax' when entry_header.enquiry_type_vendor_2 = 'others' then 'Others'
else '-' end) as enquiry_type_vendor_2,
(case when entry_header.enquiry_type_vendor_3 = 'phone' then 'Phone' when entry_header.enquiry_type_vendor_3 = 'email' then 'Email'
when entry_header.enquiry_type_vendor_3 = 'fax' then 'Fax' when entry_header.enquiry_type_vendor_3 = 'others' then 'Others'
else '-' end) as enquiry_type_vendor_3,
(case when entry_header.enquiry_type_vendor_4 = 'phone' then 'Phone' when entry_header.enquiry_type_vendor_4 = 'email' then 'Email'
