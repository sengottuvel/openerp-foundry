	<parameter name="invoice_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select  invoice_id,invoice_no,invoice_date,customer_po_no,cust_po_date,bill_address,delivery_address,


gs_tin_no,prime_cost,off_name,hsn_name,total_tax,qty,

to_char(invoice_issue_date, 'DD-MM-YYYY HH12:MI:SS AM') AS invoice_issue_date,
to_char(invoice_removal_date, 'DD-MM-YYYY HH12:MI:SS AM') AS invoice_removal_date,

vehicle_no,
place_of_supply,



trim(TO_CHAR((coalesce(total_tax,0.00)), '999G999G99G999G99G99G990D99')) as tax_gnd,

trim(TO_CHAR((coalesce(cgst_total_tax,0.00)), '999G999G99G999G99G99G990D99')) as cgst_tax_gnd,
trim(TO_CHAR((coalesce(sgst_total_tax,0.00)), '999G999G99G999G99G99G990D99')) as sgst_tax_gnd,
trim(TO_CHAR((coalesce(igst_total_tax,0.00)), '999G999G99G999G99G99G990D99')) as igst_tax_gnd,
trim(TO_CHAR((coalesce(total_before_net_amount,0.00)), '999G999G99G999G99G99G990D99')) as tot_before_tax,
trim(TO_CHAR((coalesce(total_before_net_amount+total_tax,0.00)), '999G999G99G999G99G99G990D99')) as total_net_amount,
trim(TO_CHAR((coalesce(total_before_net_amount+total_tax,0.00)), '999G999G99G999G99G99G990D99')) as net_after_tax,

case when total_before_net_amount+total_tax > 0 then
(select f_amount_to_words((total_before_net_amount+total_tax)::numeric,1)) else '' end as word_total_inv,

case when cgst_total_tax > 0 then
(select f_amount_to_words((cgst_total_tax)::numeric,1)) else '' end as word_cgst_amt,
case when sgst_total_tax > 0 then
(select f_amount_to_words((sgst_total_tax)::numeric,1)) else '' end as word_sgst_amt,
case when igst_total_tax > 0 then
(select f_amount_to_words((igst_total_tax)::numeric,1)) else '' end as word_igst_amt

from

(


select
sale.id as invoice_id,
case when sale.invoice_no is not null then
sale.invoice_no else '' end as invoice_no,
case when sale.invoice_date is not null then
to_char(sale.invoice_date,'dd-mm-yyyy') else '' end as invoice_date,
case when sale.customer_po_no is not null then
sale.customer_po_no else '' end as customer_po_no,
case when sale.cust_po_date is not null then
to_char(sale.cust_po_date,'dd-mm-yyyy') else '' end as cust_po_date,
sale.bill_address as bill_address,
sale.delivery_address as delivery_address,
sale.invoice_issue_date as invoice_issue_date,



sale.invoice_removal_date as invoice_removal_date,
sale.vehicle_no as vehicle_no,
sale.place_of_supply as place_of_supply,


case when partner.gs_tin_no is not null then
partner.gs_tin_no else '' end as gs_tin_no,
invoice_copy.taxable_value as prime_cost,
case when invoice_copy.description is not null then
invoice_copy.description else '' end as off_name,
hsn.name as hsn_name,
invoice_copy.qty as qty,

(select sum(cgst_amt + sgst_amt + igst_amt) from ch_customer_invoice_copy_rpt where header_id = sale.id)  as total_tax,

(select sum(cgst_amt) from ch_customer_invoice_copy_rpt where header_id = sale.id)  as cgst_total_tax,

(select sum(sgst_amt) from ch_customer_invoice_copy_rpt where header_id = sale.id)  as sgst_total_tax,

(select sum(igst_amt) from ch_customer_invoice_copy_rpt where header_id = sale.id)  as igst_total_tax,

(select sum(total_taxable_value) from ch_customer_invoice_copy_rpt where header_id = sale.id)  as total_before_net_amount


from

kg_sale_invoice sale
left join ch_customer_invoice_copy_rpt invoice_copy on (invoice_copy.header_id = sale.id)
left join kg_hsn_master hsn on (hsn.id = invoice_copy.hsn_id)
left join res_partner partner on (partner.id = sale.customer_id)
left join res_country_state state on(state.id=partner.state_id)
left join res_company company on (company.id = sale.company_id)

where invoice_copy.header_id=$P{invoice_id} limit 1 ) as final]]>
	</queryString>
