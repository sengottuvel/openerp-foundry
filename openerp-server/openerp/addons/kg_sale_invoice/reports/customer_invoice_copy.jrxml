	<subDataset name="dataset1">
		<parameter name="invoice_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
sale.id as invoice_id,
case when sale.invoice_no is not null then
sale.invoice_no else '' end as invoice_no,
invoice_copy.description as off_name,
hsn.name as hsn_name,
invoice_copy.qty as qty,
trim(TO_CHAR((coalesce(round(invoice_copy.unit_price),0.00)), '999G999G99G999G99G99G990D99')) as prime_cost,
trim(TO_CHAR((coalesce(round(invoice_copy.deductions),0.00)), '999G999G99G999G99G99G990D99')) as deductions,
trim(TO_CHAR((coalesce(round(invoice_copy.additions),0.00)), '999G999G99G999G99G99G990D99')) as additions,
trim(TO_CHAR((coalesce(round(invoice_copy.taxable_value),0.00)), '999G999G99G999G99G99G990D99')) as tax_tot,
trim(TO_CHAR((coalesce(round(invoice_copy.total_taxable_value),0.00)), '999G999G99G999G99G99G990D99')) as net_amount,
trim(TO_CHAR((coalesce(round(invoice_copy.cgst_amt),0.00)), '999G999G99G999G99G99G990D99')) as cgst_tax_val,
trim(TO_CHAR((coalesce(round(invoice_copy.sgst_amt),0.00)), '999G999G99G999G99G99G990D99')) as sgst_tax_val,
trim(TO_CHAR((coalesce(round(invoice_copy.igst_amt),0.00)), '999G999G99G999G99G99G990D99')) as igst_tax_val,
invoice_copy.cgst_per as cgst_rate,
invoice_copy.sgst_per as sgst_rate,
invoice_copy.igst_per as igst_rate

from

kg_sale_invoice sale
left join ch_customer_invoice_copy_rpt invoice_copy on (invoice_copy.header_id = sale.id)
left join kg_hsn_master hsn on (hsn.id = invoice_copy.hsn_id)
left join res_partner partner on (partner.id = sale.customer_id)
left join res_country_state state on(state.id=partner.state_id)
left join res_company company on (company.id = sale.company_id)

where invoice_copy.header_id=$P{invoice_id}]]>
		</queryString>
