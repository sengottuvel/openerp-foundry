(select count(id) from hr_holidays where from_date >= monthly_att.start_date and to_date <= monthly_att.end_date and holiday_status_id=23 and employee_id=payslip.employee_id) as cl_days,
(select count(id) from hr_holidays where from_date >= monthly_att.start_date and to_date <= monthly_att.end_date and holiday_status_id=24 and employee_id=payslip.employee_id) as el_days

