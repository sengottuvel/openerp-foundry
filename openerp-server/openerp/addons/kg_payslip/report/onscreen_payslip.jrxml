from hr_payslip as payslip

left join hr_employee employee on (employee.id= payslip.employee_id)
left join hr_contract contract on (contract.employee_id= payslip.employee_id)
left join ch_kg_contract_salary contract_line on (contract_line.header_id_salary= contract.id)
left join hr_salary_rule salary_comp on (salary_comp.id= contract_line.salary_type)
left join kg_depmaster department on (department.id= employee.dep_id)
left join kg_division_master division on (division.id= employee.division_id)
left join hr_job designation on (designation.id= employee.job_id)
left join res_bank bank on (bank.id= contract.bank_id)
left join kg_monthly_attendance monthly_att on (monthly_att.id = (select id from kg_monthly_attendance where start_date=payslip.date_from and end_date=payslip.date_to and employee_id=payslip.employee_id
and state='approved' limit 1))

where payslip.id =$P{payslip_id} ) as a limit 1) as b]]>
