COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='CLOTH'),0.00)as cloth,

-----DEDUCTION ENDS-------

-----ACTUAL SALARY STARTS-------

case when salary_comp.code ='BASIC' then to_char(contract_line.salary_amt,'9G99G990D99') else '0.00' end actual_basic_text,
