to_char(COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='SA'),0.00),'9G99G990D99') as earned_spl_allow_text,
to_char(COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='MA'),0.00),'9G99G990D99') as earned_ma_text,
to_char(COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='ATTN. ALLO'),0.00),'9G99G990D99') as earned_att_allow_text,


COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='BASIC'),0.00)as earned_basic,
COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='VDA'),0.00)as earned_vda,
COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='FDA'),0.00)as earned_fda,
(COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='BASIC'),0.00) + COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='VDA'),0.00)+
COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='FDA'),0.00)) as pf_salary,
COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='HRA'),0.00)as earned_hra,
COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='CA'),0.00)as earned_ca,
COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='SA'),0.00)as earned_spl_allow,
COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='MA'),0.00)as earned_ma,
COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='ATTN. ALLO'),0.00)as earned_att_allow,

-----EARNED SALARY ENDS-------

-----DEDUCTION STARTS-------

to_char(COALESCE( (select amount from hr_payslip_line where slip_id = payslip.id and code='PF'),0.00),'9G99G990D99') pf_text,
