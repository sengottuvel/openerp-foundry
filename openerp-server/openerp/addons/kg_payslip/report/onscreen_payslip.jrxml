--case when salary_comp.code ='HRA' then to_char(contract_line.salary_amt,'9G99G990D99') else '0.00' end actual_hra_text,
--case when salary_comp.code ='CA' then to_char(contract_line.salary_amt,'9G99G990D99') else '0.00' end actual_ca_text,
--case when salary_comp.code ='SA' then to_char(contract_line.salary_amt,'9G99G990D99') else '0.00' end actual_sa_text,
