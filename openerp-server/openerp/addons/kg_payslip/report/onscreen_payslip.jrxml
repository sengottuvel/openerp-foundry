	<subDataset name="dataset1">
		<queryString>
			<![CDATA[select ss.component,ss.amount,ss.actual_amt from (

select

payslip_line.name as component,
payslip_line.amount as amount,
0 as actual_amt
from hr_payslip payslip

join hr_payslip_line payslip_line on (payslip_line.slip_id = payslip.id)

where payslip.id=240

union

select

other_payslip_line.name as component,
other_payslip_line.amount as amount,
0 as actual_amt

from hr_payslip payslip

join ch_other_salary_comp other_payslip_line on (other_payslip_line.slip_id = payslip.id)

where payslip.id=240

union

select

salary_comp.name as component,
0 as amount,
contract_sal_line.salary_amt as actual_amt


from hr_payslip payslip

left join hr_contract contract on (contract.employee_id = payslip.employee_id)
left join ch_kg_contract_salary contract_sal_line on (contract_sal_line.header_id_salary = contract.id)
left join hr_salary_rule salary_comp on (salary_comp.id = contract_sal_line.salary_type)

where payslip.id=240) as ss]]>
		</queryString>
