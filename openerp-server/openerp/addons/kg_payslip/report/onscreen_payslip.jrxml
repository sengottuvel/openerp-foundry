else to_char((select count(id) from hr_holidays where from_date >= monthly_att.start_date and to_date <= monthly_att.end_date and holiday_status_id=23 and employee_id=payslip.employee_id),'99D9') end as cl_days_text,

case when (select count(id) from hr_holidays where from_date >= monthly_att.start_date and to_date <= monthly_att.end_date and holiday_status_id=23 and employee_id=payslip.employee_id) = 0.0 then '0.0'
else to_char((select count(id) from hr_holidays where from_date >= monthly_att.start_date and to_date <= monthly_att.end_date and holiday_status_id=24 and employee_id=payslip.employee_id),'99D9') end as el_days_text,

monthly_att.working_days as working_days,
