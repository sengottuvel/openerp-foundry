	<subDataset name="dataset1"/>
	<subDataset name="New Dataset 1">
		<parameter name="order_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
so_line.product_id as product_id,
pro_temp.name ||
		(CASE 	WHEN brand.name!=''
			THEN  ' | ' || brand.name
			ELSE ''
		END ) as product_name,
pro_uom.name as product_uom,
so_line.product_qty as so_qty,
trim(TO_CHAR(so_line.price_unit::float, '999G999G99G999G99G99G990D99'))  as so_rate,
trim(TO_CHAR((so_line.product_qty * so_line.price_unit)::float, '999G999G99G999G99G99G990D99'))  as value,
so_line.kg_discount_per as discount,
case when so_line.id is not null then
(SELECT CAST(string_agg(description, ',') AS text)
FROM account_tax where id in (select service_order_line_id from service_order_tax where tax_id=so_line.id))
end as tax_name

from kg_service_order_line so_line

left join product_template pro_temp on (pro_temp.id = so_line.product_id)
left join product_uom pro_uom on (pro_uom.id = so_line.product_uom)
left join kg_brand_master brand on (brand.id = so_line.brand_id)

where so_line.service_id = $P{order_id} order by so_line.id]]>
		</queryString>
		<field name="product_id" class="java.lang.Integer"/>
		<field name="product_name" class="java.lang.String"/>
		<field name="product_uom" class="java.lang.String"/>
		<field name="so_qty" class="java.lang.Double"/>
		<field name="so_rate" class="java.lang.String"/>
		<field name="value" class="java.lang.String"/>
		<field name="discount" class="java.math.BigDecimal"/>
		<field name="tax_name" class="java.lang.String"/>
	</subDataset>
	<subDataset name="dataset2"/>
	<parameter name="so_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
so.id as id,
comp.name as company_name,
part.street as street1,
part.street2 as street2,
comp_city.name as city,
comp_state.name as state,
comp_country.name as country,
part.zip as zip,
part.phone as phone,
part.fax as fax,
part.email as email,
part.website as website,
CASE WHEN part.tin_no != '' THEN 'TIN NO :'
ELSE ''
END as tin_label,
part.tin_no as tin_no,
CASE WHEN comp.rml_header1 = null THEN null
ELSE comp.rml_header1
END as tagline,
so.name as so_name,
to_char(so.date::date,'dd-mm-YYYY') as so_date,
partner.name as supplier_name,
partner.street as sup_street1,
partner.street2 as sup_street2,
partner.contact_person as contact_person,
partner.phone as sup_phone,
partner.fax as sup_fax,
partner.email as sup_email,
partner_city.name as sup_city,
partner_state.name as sup_state,
partner_country.name as sup_country,
partner.zip as sup_zip,
partner.mobile as sup_mobile,
so.quot_ref_no as quot_ref_no,
so.version as version,
trim(TO_CHAR(so.amount_untaxed::float, '999G999G99G999G99G99G990D99'))  as total_amt,
trim(TO_CHAR(so.discount::float, '999G999G99G999G99G99G990D99'))  as discount,
trim(TO_CHAR(so.amount_tax::float, '999G999G99G999G99G99G990D99')) as tax,
trim(TO_CHAR(so.amount_total::float, '999G999G99G999G99G99G990D99'))   as so_amt,
trim(TO_CHAR((so.value1 + so.value2)::float, '999G999G99G999G99G99G990D99'))  as other_charge,
payment.name as payment,
delivery.name as delivery,
so.freight_charges as frieght,
CASE WHEN so.price='inclusive' THEN 'Inclusive of all taxes and duties'
ELSE 'Exclusive of all taxes and duties'
END as price,
f_amount_to_words(so.amount_total,so.company_id) as amount_in_words,
so.specification as specification,
so.origin as project,
so.warranty as warranty,
so.quot_ref_no as quot_ref_no,
so.state as status

from kg_service_order so

left join res_company comp on (comp.id = so.company_id)
left join res_partner part on (part.id = comp.partner_id)
left join res_partner partner on (partner.id = so.partner_id)
left join res_city comp_city on (comp_city.id = part.city_id)
left join res_city partner_city on (partner_city.id = partner.city_id)
left join res_country_state comp_state on (comp_state.id = part.state_id)
left join res_country_state partner_state on (partner_state.id = partner.state_id)
left join res_country comp_country on (comp_country.id = part.country_id)
left join res_country partner_country on (partner_country.id = partner.country_id)
left join kg_payment_master payment on (payment.id = so.payment_mode)
left join kg_delivery_master delivery on (delivery.id = so.delivery_mode)

where so.id = $P{so_id}]]>
	</queryString>
